<?xml version="1.0" encoding="utf-8" ?>
<!-- Suppresses log output for tests -->
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>

  <log4net>
    <root>
      <level value="ALL" />
    </root>
  </log4net>
</configuration>